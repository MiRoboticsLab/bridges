geometry_msgs/Twist twist               # 运动速度
geometry_msgs/Point centroid_height     # 质心高度(米)
geometry_msgs/Point right_forefoot      # 右前足:落脚点位置
geometry_msgs/Point left_forefoot       # 左前足:落脚点位置
geometry_msgs/Point right_hindfoot      # 右后足:落脚点位置
geometry_msgs/Point left_hindfoot       # 左后足:落脚点位置
float32 forefoot_height                 # 前足:抬脚高度(米)
float32 hindfoot_height                 # 后足:抬脚高度(米)
float32 friction_coefficient            # 摩擦系数
int64 duration_ms                       # 持续时间(毫秒)
